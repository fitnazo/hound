<p id="notice"><%= notice %></p>

<h1>Listing Job Schedulers</h1>
Time: <%= Time.now.localtime %>
<table>
  <thead>
    <tr>
      <th>Event at</th>
      <th>Event at Local</th>
      <th>Frequency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_schedulers.each do |job_scheduler| %>
      <tr>
        <td><%= job_scheduler.event_at %></td>
        <td>| <%= job_scheduler.event_at.localtime %></td>
        <td><%= job_scheduler.frequency %></td>
        <td><%= link_to 'Show', job_scheduler %></td>
        <td><%= link_to 'Edit', edit_job_scheduler_path(job_scheduler) %></td>
        <td><%= link_to 'Destroy', job_scheduler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job scheduler', new_job_scheduler_path %>
